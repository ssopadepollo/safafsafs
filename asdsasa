    getgenv().magnitudepaso = t
end)

Farm:Toggle("Harverest",function(t)
    getgenv().hplant = t
    spawn(function()
        while getgenv().hplant == true and wait(1) do
            for i, v in pairs(game.Workspace:GetChildren()) do
                if v.Name == getgenv().paso then
                for i, c in pairs(v:GetChildren()) do
                if c:IsA("Part") then
                if (c.Position - game:GetService("Players").LocalPlayer.Character.UpperTorso.Position).magnitude < getgenv().magnitudepaso then
                game:GetService("ReplicatedStorage").Events.Pickup:FireServer(v)
                end
                end
                end
                end
                end
        end
    end)
end)

Farm:Toggle("Make Coin / To the nearest Coin Press",function(t)
    getgenv().makecoin = t
    spawn(function()
        while getgenv().makecoin == true and wait() do
            for i=1, 5 do
                for i, v in pairs(game.Workspace.Deployables:GetChildren()) do
                if v.Name == "Coin Press" then
                if (v.PrimaryPart.Position - game:GetService("Players").LocalPlayer.Character.UpperTorso.Position).magnitude < 20 then
                local item = "Gold"
                game:GetService("ReplicatedStorage").Events.lnteractStructure:FireServer(v, item)
                game:GetService("ReplicatedStorage").Events.lnteractStructure:FireServer(v, item)
                game:GetService("ReplicatedStorage").Events.lnteractStructure:FireServer(v, item)
                game:GetService("ReplicatedStorage").Events.lnteractStructure:FireServer(v, item)
                game:GetService("ReplicatedStorage").Events.lnteractStructure:FireServer(v, item)

                wait(0.001)
                end
                end
                end
                end
        end
    end)
end)



Farm:Label("TP Points")

Farm:Toggle("Rec TP",function(t)
    getgenv().tp1c = t
    spawn(function()
            _G.SetFarmPos = t
	     
            if _G.SetFarmPos then
                print("wsp")
                n = 1
           _G.farmarray = {}
            end
           while _G.SetFarmPos and wait(.5) do
               print(n)
           _G.farmarray[n] =  game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
           n = n + 1 
           end
    end)
end)

Farm:Toggle("Start TP Points",function(Value)
            _G.autofarmfruit = Value
            _G.farmarraylength = #(_G.farmarray)
                print("Farm array length = ", farmarraylength)
            while _G.autofarmfruit and wait() do
    for i = 1, _G.farmarraylength do
        if _G.autofarmfruit == false then
            i = 0 
            _G.farmarraylength = 0
        end
        
        print(i)
        print(_G.farmarraylength)
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = _G.farmarray[i]
        wait(.8)
    
    end  
    end
end)

