--------------------------------------------------------------------------------------------------------MAP SECTION---------------------------

Mp:Button("Max Fog",function()
    while wait() do
    game:GetService("Lighting").FogEnd = 200000
    game:GetService("Lighting").FogStart = 200000
    end
end)

Mp:Button("Default Map",function()
    game:GetService("Lighting").FogEnd = 2000
    game:GetService("Lighting").FogStart = 100
    game:GetService("Lighting").ColorCorrection.Saturation = 0.2
    game:GetService("Lighting").Blur.Size = 12
    game:GetService("Lighting").Bloom.Size = 12
    game:GetService("Lighting").Brightness = 3
    local time = 14
    game:GetService("Lighting").TimeOfDay = time..":00:00"
end)

Mp:Button("Day",function()
    local time1 = 14
    game:GetService("Lighting").TimeOfDay = time1..":00:00"
end)

Mp:Button("Night",function()
    local time2 = 00
    game:GetService("Lighting").TimeOfDay = time2..":00:00"
end)

Mp:Slider("Color Correction", 0.2, 1, 0.2, function(t)
    game:GetService("Lighting").ColorCorrection.Saturation = t
end)

Mp:Slider("Bloom", 0, 100, 12, function(t)
    game:GetService("Lighting").Bloom.Size = t
end)

Mp:Slider("Brightness", 0, 10, 3, function(t)
    game:GetService("Lighting").Brightness = t
end)

Mp:Slider("Time", 0, 24, 12, function(t)
    game:GetService("Lighting").TimeOfDay = t..":00:00"
end)

Mp:Button("Day Shader",function()
    local time12 = 7
    game:GetService("Lighting").TimeOfDay = time12..":00:00"
    game:GetService("Lighting").Brightness = 5
    game:GetService("Lighting").ExposureCompensation = 0.1
-----------------STRUCTURES COLORED----------------------------
for i, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Crystal Lode" then
        for i, c in pairs(v:GetDescendants()) do
            if c:IsA("UnionOperation") or c:IsA("Part") then
                c.Transparency = 0.5
            end
        end
    end
end
for i, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Ice Chunk" then
        for i, c in pairs(v:GetDescendants()) do
            if c:IsA("UnionOperation") or c:IsA("Part") then
                c.Material = "Glass"
                c.Transparency = 0.5
                for i, v in pairs(game.Workspace:GetDescendants()) do
                                    c.Color = Color3.fromRGB(242, 243, 243)
                end
        end
        end
    end
end
for i, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Gold Node" then
        for i, c in pairs(v:GetDescendants()) do
            if c:IsA("UnionOperation") or c:IsA("Part") then
                c.Material = "Metal"
                c.Transparency = 0
                c.Color = Color3.fromRGB(255, 176, 0)
                end
        end
        end
end
for i, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Iceberg" then
        for i, c in pairs(v:GetDescendants()) do
            if c:IsA("UnionOperation") or c:IsA("Part") then
                c.Material = "Glass"
                c.Transparency = 0.5
                c.Color = Color3.fromRGB(242, 243, 243)
                for i, v in pairs(game.Workspace:GetDescendants()) do
                end
        end
        end
    end
end
for i, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Frozen Barbarian" then
        for i, c in pairs(v:GetDescendants()) do
            if c:IsA("UnionOperation") or c:IsA("Part") then
                c.Material = "Glass"
                c.Transparency = 0.5
                c.Color = Color3.fromRGB(242, 243, 243)
    
        end
        end
    end
end
for i, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Frozen Giant" then
        for i, c in pairs(v:GetDescendants()) do
            if c:IsA("UnionOperation") or c:IsA("Part") then
                c.Material = "Glass"
                c.Transparency = 0.5
                c.Color = Color3.fromRGB(242, 243, 243)
    
        end
        end
    end
end
game.Workspace.Terrain.WaterColor = Color3.fromRGB(87, 109, 131)
end)

