Farm:Textbox("Fuel Name | Wood, Leave, Log, Coal", true,function(t)
    getgenv().itemfuel3 = t
    notif("Added Fuel | " .. t, 3)
end)

Farm:Textbox("Auto Fuel (<)", true,function(t)
    getgenv().itemfuel22 = t
    notif("Added > " .. t, 3)
end)

Farm:Toggle("Auto Fuel",function(t)
    getgenv().autofueal = t
    spawn(function()
        while getgenv().autofueal == true and wait() do
            for i, v in pairs(game:GetService("Workspace").Deployables:GetChildren()) do
                if v.Name == "Campfire" then
                    if (v.PrimaryPart.Position - game:GetService("Players").LocalPlayer.Character.UpperTorso.Position).magnitude < 60 then
            local camp = v.Board.Billboard.Backdrop.TextLabel
            if camp.Text < getgenv().itemfuel22 then
            game:GetService("ReplicatedStorage").Events.lnteractStructure:FireServer(v, getgenv().itemfuel3)
            end
            end
            end
            end
        end
    end)
end)

PlantB:Button("1 x 1",function()
    local LocalPlayer = game:GetService("Players").LocalPlayer
    local RootPart = LocalPlayer.Character.HumanoidRootPart
    local cframe = RootPart.CFrame
    local Remote = game:GetService("ReplicatedStorage").Events.PlaceStructure
    
    Remote:FireServer("Plant Box", cframe * CFrame.new(-1, -2.5, 1), 2)
end)

