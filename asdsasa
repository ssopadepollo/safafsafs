local VLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/vep1032/VepStuff/main/VL"))()
local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
local inviteModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Discord%20Inviter/Source.lua"))()
local s = VLib:Window("EzP Gui", "Booga Reborn", "Yes")
local playerService = game:GetService("Players")
local localPlayer = playerService.LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")
local workspace = game:GetService("Workspace")
getgenv().notifColor = Color3.fromRGB(255, 0, 0)
local function notif(text, duration)
    local notifModule = getsenv(localPlayer.PlayerScripts["Local Handler"])
    notifModule.functionBank.CreateNotification(text, getgenv().notifColor, duration)
end
--TABS-------------------------------------------------------------------------------------------------
-------------------------------------------------------------------------------------------------------
local TP = s:Tab("Teleport")
local Main = s:Tab("Main")
local Heals = s:Tab("Heals")
local Comb = s:Tab("Combat")
local Pl = s:Tab("Player")
local Farm = s:Tab("Farm")
local PlantB = s:Tab("Plant - Boxes")
local ESP = s:Tab("ESP")
local Inv = s:Tab("Invisible")
local Pick = s:Tab("Pickup Whitelist")
local St = s:Tab("Structure")
local Mp = s:Tab("Map")
local dn = s:Tab("Donators")


--eMPIEZA-------------------------------------------------------------------------------------------------
-------------------------------------------------------------------------------------------------------

TP:Button("Old God",function()
    local me = game.Players.LocalPlayer.Character
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(218.935, -140.998, 1079.8)
    me.Humanoid.Jump = true
end)

TP:Button("Volcano",function()
    local me = game.Players.LocalPlayer.Character
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1064, -196, -427)
    me.Humanoid.Jump = true
end)

TP:Button("Void TP",function()
    local TeleportService = game:GetService('TeleportService')
    local placeID = 11879754496
    local player = game.Players.LocalPlayer
    TeleportService:Teleport(placeID, player)
end)

TP:Button("Copy Void Link",function()
    local cop = "https://web.roblox.com/games/11879754496/Voidness"
    setclipboard(tostring(cop))
end)

TP:Toggle("Touch TP | Sky Islands",function(t)
    getgenv().tpares = t
    spawn(function()
        if getgenv().tpares then
            wait()
            pcall(
                function()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/ssopadepollo/seriuslybro/main/wsp", true))()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/ssopadepollo/puro1/main/arbolggiante", true))()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/ssopadepollo/crystalicaca/main/ohhyeahhh", true))()
end)
elseif not getgenv().tpares then
for i, v in pairs(game.Workspace:GetChildren()) do
    if v.Name == "CrystalPart" or v.Name == "AncientPart" or v.Name == "SkyPart" then
        v:Destroy()
    end
end
end
end)
end)


TP:Textbox("Player", true,function(t)
    getgenv().vgoto = t
        notif("+Added " .. t, 3)
    end)

TP:Button("Boat Goto",function()
    local seat = game.Players.LocalPlayer.Character.Humanoid.SeatPart
    local raft = seat.Parent
    local enemy = getgenv().vgoto
    local me = game.Players.LocalPlayer.Character
            local posenemy = game.Players[enemy].Character.Head.Position
            me.HumanoidRootPart.Anchored = true
            me.Head.Anchored = true
            me.LowerTorso.Anchored = true
            raft:MoveTo(posenemy)
    wait(1)
    local me = game.Players.LocalPlayer.Character
    me.Humanoid.Jump = true
            me.HumanoidRootPart.Anchored = false
            me.Head.Anchored = false
            me.LowerTorso.Anchored = false    
end)

TP:Label("Bring To Me")

TP:Toggle("Bring Boats (Near)",function(t)
    getgenv().bringcrittersc = t
    spawn(function()
        if getgenv().bringcrittersc then
            wait()
            pcall(
                function()
                    local mepos = game.Players.LocalPlayer.Character.Head.Position
                    for i, v in pairs(game.Workspace.Deployables:GetChildren()) do
                        if v.Name:find("Raft") or v.Name:find("Sailboat") or v.Name:find("Boat") then
                    for i, c in pairs(v:GetDescendants()) do
                        if c:IsA("Part") or c:IsA("UnionOperation") or c:IsA("VehicleSeat") then
                                          local ForceInstance = Instance.new("BodyPosition")
                                        ForceInstance.Parent = c
                                        ForceInstance.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                                        ForceInstance.D = 1500
                                        ForceInstance.P = 25000
                                        ForceInstance.Position = mepos
                    end
                    end      
                        end
                end            
                end)
        elseif not getgenv().bringcrittersc then
            local mepos = game.Players.LocalPlayer.Character.Head.Position
            for i, v in pairs(game.Workspace.Deployables:GetChildren()) do
                if v.Name:find("Raft") or v.Name:find("Sailboat") or v.Name:find("Boat") then
            for i, c in pairs(v:GetDescendants()) do
                if c:IsA("Part") or c:IsA("UnionOperation") or c:IsA("VehicleSeat") then
                    for i, d in pairs(c:GetDescendants()) do
                        if d:IsA("BodyPosition") then
                            d:Destroy()

            end
                    end    
        
    end
                end
                end
    end
end
    end)
end)

TP:Toggle("Bring Items (Near)",function(t)
    getgenv().bring = t
    spawn(function()
        if getgenv().bring then
            wait()
            pcall(
                function()
                    local mepos = game.Players.LocalPlayer.Character.Head.Position
                    for i, v in pairs(game.Workspace.Items:GetChildren()) do
                        if v:IsA("Part") then
                    v.Anchored = false
                                          local ForceInstance = Instance.new("BodyPosition")
                                        ForceInstance.Parent = v
                                        ForceInstance.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                                        ForceInstance.D = 1500
                                        ForceInstance.P = 25000
                                        ForceInstance.Position = mepos
                    end
                    end                    
                end)
        elseif not getgenv().bring then
            for i, v in pairs(game.Workspace.Items:GetChildren()) do
                if v:IsA("Part") then
                    for i, c in pairs(v:GetDescendants()) do
                        if c:IsA("BodyPosition") then
                            c:Destroy()
                        end
                    end
                end
            end            
        end
    end)
end)

TP:Toggle("Bring Critters (Near)",function(t)
    getgenv().bringcritters = t
    spawn(function()
        if getgenv().bringcritters then
            wait()
            pcall(
                function()
                    local mepos = game.Players.LocalPlayer.Character.Head.Position
                    for i, v in pairs(game.Workspace.Critters:GetDescendants()) do
                        if v:IsA("Model") then
                    for i, d in pairs(v:GetDescendants()) do
                        if d:IsA("UnionOperation") or d:IsA("Part") then 
                            d.Anchored = false
                                          local ForceInstance = Instance.new("BodyPosition")
                                        ForceInstance.Parent = d
                                        ForceInstance.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                                        ForceInstance.D = 1500
                                        ForceInstance.P = 25000
                                        ForceInstance.Position = mepos
                    end
                        end    
                    end
            end                
                end)
        elseif not getgenv().bringcritters then
            local mepos = game.Players.LocalPlayer.Character.Head.Position
            for i, v in pairs(game.Workspace.Critters:GetDescendants()) do
                if v:IsA("Model") then
            for i, d in pairs(v:GetDescendants()) do
                if d:IsA("UnionOperation") or d:IsA("Part") then 
                    for i, o in pairs(d:GetDescendants()) do
                        if o:IsA("BodyPosition") then
                            o:Destroy()
                        end
                    end
                end
            end
                end
        end        
        end
    end)
end)

TP:Label("Bring To Players")

