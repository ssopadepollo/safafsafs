---------------------------BRING TO PLAYER


Main:Label("Welcome to EzP Gui")
Main:Label("Toggle GUI with Right Ctrl")
Main:Button("Destroy GUI",function()
    game:GetService("CoreGui").Library:Destroy()
end)
Main:Button("Copy Discord Link",function()
    local copy = "https://discord.gg/rudE7ucZdx"
    setclipboard(tostring(copy))
    inviteModule.Join("https://discord.gg/rudE7ucZdx")
end)
Main:Colorpicker("Notification Color",Color3.fromRGB(255, 0, 0),function(t)
    getgenv().notifColor = t
end)
Main:Button("Credits",function()
    Notification:Notify(
        {Title = "Credits", Description = "RatatuiHelp#0267 (me)"},
        {OutlineColor = getgenv().notifColor,Time = 5, Type = "default"}
    )
end)

Main:Colorpicker("Selection Color",Color3.fromRGB(255, 0, 0),function(t)
    getgenv().slco = t
end)

Main:Toggle("Selection",function(t)
    getgenv().sc = t
    spawn(function()
        while getgenv().sc == true and wait() do
            game:GetService("Workspace").SelectionBox.Color3 = getgenv().slco
        end
    end)
end)

Main:Toggle("Hide Name List",function(t)
    getgenv().tplantc = t
    spawn(function()
        while getgenv().tplantc == true and wait() do
            local plr = game.Players.LocalPlayer
            local plrr = game.Players.LocalPlayer.Name
            game:GetService("Players").LocalPlayer.PlayerGui.SecondaryGui.PlayerList.List[plrr].Visible = false
        end
    end)
end)

Main:Toggle("Hide Lvl",function(t)
    getgenv().goldspa = t
    spawn(function()
        if getgenv().goldspa then
            wait()
            pcall(
                function()
                    game:GetService("Players").RatatuiHelp.PlayerGui.MainGui.Panels.Topbar.EssenceBar.TextLabel.Visible = false
                end)
        elseif not getgenv().goldspa then
            game:GetService("Players").RatatuiHelp.PlayerGui.MainGui.Panels.Topbar.EssenceBar.TextLabel.Visible = true
        end
    end)
end)

Main:Textbox("Quantity", true,function(t)
getgenv().cantidad = t
    notif("+Added " .. t.."Quantity", 3)
end)

Main:Textbox("Item", true,function(t)
    getgenv().cantidad1 = t
        notif("+Added " .. t, 3)
end)

Main:Toggle("Fake Item Quantity",function(t)
    getgenv().tc = t
    spawn(function()
        while getgenv().tc == true and wait() do
            game:GetService("Players").LocalPlayer.PlayerGui.MainGui.RightPanel.Inventory.List[getgenv().cantidad1].QuantityImage.QuantityText.Text = getgenv().cantidad
        end
    end)
end)

Main:Colorpicker("Color",Color3.fromRGB(255, 0, 0),function(t)
    getgenv().slcos = t
end)

Main:Toggle("Armor Color (Fake)",function(t)
    getgenv().ssc = t
    spawn(function()
        while getgenv().ssc == true and wait() do
            local plr = game.Players.LocalPlayer.Character
            for i, v in pairs(plr:GetDescendants()) do
            if v.Name:find("Chestplate") or v.Name:find("Leg") or v.Name:find("Shoulder") or v.Name:find("Foot") or v.Name:find("Halo") then
            for i, c in pairs(v:GetDescendants()) do
            if c:IsA("Part") then
            c.Color = getgenv().slcos
            end
            end
            end
            end
        end
    end)
end)

Main:Colorpicker("Color",Color3.fromRGB(255, 0, 0),function(t)
    getgenv().slcoss = t
end)

Main:Toggle("Bag Color (Fake)",function(t)
    getgenv().sscs = t
    spawn(function()
        while getgenv().sscs == true and wait() do
            local plr = game.Players.LocalPlayer.Character
            for i, v in pairs(plr:GetDescendants()) do
            if v.Name:find("Bag") then
            for i, c in pairs(v:GetDescendants()) do
            if c:IsA("Part") then
            c.Color = getgenv().slcoss
            end
            end
            end
            end
        end
    end)
end)

