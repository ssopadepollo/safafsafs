Heals:Toggle("Auto Heal",function(t)
    getgenv().myautoheal = t
    spawn(function()
        while getgenv().myautoheal == true and wait() do
            function food()
                for i=1, getgenv().nospeed do
                game:GetService("ReplicatedStorage").Events.UseBagItem:FireServer(getgenv().nono) 
                end
                end
                if game.Players.LocalPlayer.Character.Humanoid.Health < 100 then
                food()
                end
        end
    end)
end)

--COMBAT--------------------------------------------------------------------------------------------

Comb:Toggle("Kill Aura",function(t)
    getgenv().tpclaant = t
    spawn(function()
        while getgenv().tpclaant == true and wait() do
            wait()
            pcall(
                function()
                    local p = game:GetService("Players")
                    local n = p.LocalPlayer.Character.HumanoidRootPart
                    local q = game:GetService("Workspace")
                    local o = game:GetService("ReplicatedStorage")
                    function getNear()
                        local R
                        local S = 100, nil
                        for b, c in pairs(p:GetPlayers()) do
                            if c ~= p.LocalPlayer and c.Character.HumanoidRootPart then
                                if
                                    (n.Position - c.Character.HumanoidRootPart.Position).Magnitude <
                                        S
                                 then
                                    R = c.Character.HumanoidRootPart
                                    S =
                                        (n.Position - c.Character.HumanoidRootPart.Position).Magnitude
                                end
                            end
                        end
                        return R
                    end
                    local U = getNear()
                    local ac = {}
                    for b, c in pairs(U.Parent:GetChildren()) do
                        if c:IsA("BasePart") then
                            table.insert(ac, c)
                        end
                    end
                    game:GetService("ReplicatedStorage").Events.SwingTool:FireServer(
                        o.RelativeTime.Value,
                        ac
                    )
                end
            )
        end
    end)
end)

Comb:Toggle("Show Aura Hitbox",function(t)
    getgenv().ancsct = t
    spawn(function()
        if getgenv().ancsct then
            wait()
            pcall(
                function()
                    for i,v in next, game:GetService('Players'):GetPlayers() do
                        if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                        pcall(function()
                        v.Character.HumanoidRootPart.Size = Vector3.new(13, 13, 13)
                        v.Character.HumanoidRootPart.Transparency = 0.4
                        v.Character.HumanoidRootPart.Color = Color3.fromRGB(77, 0, 0)
                        v.Character.HumanoidRootPart.Material = "Block"
                        v.Character.HumanoidRootPart.CanCollide = false
                        end)
                        end
                        end
                end
            )
            wait(5)
        elseif not getgenv().ancsct then
            for i,v in next, game:GetService('Players'):GetPlayers() do
                v.Character.HumanoidRootPart.Transparency = 1
                v.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
                end
            end
    end)
end)

Comb:Slider("Range", 8,13,11, function(t)
   getgenv().hitboxsl = t
 end)
 
 Comb:Toggle("Hitbox Extender",function(t)
    getgenv().ancsct = t
    spawn(function()
        if getgenv().ancsct then
            wait()
            pcall(
                function()

                    for i,v in next, game:GetService('Players'):GetPlayers() do
                        if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                        pcall(function()
                        v.Character.HumanoidRootPart.Size = Vector3.new(getgenv().hitboxsl, getgenv().hitboxsl, getgenv().hitboxsl)
                        v.Character.HumanoidRootPart.Transparency = 0.4
                        v.Character.HumanoidRootPart.Color = Color3.fromRGB(77, 0, 0)
                        v.Character.HumanoidRootPart.Material = "Block"
                        v.Character.HumanoidRootPart.CanCollide = false
                        end)
                        end
                        end
                end
            )
            wait(5)
        elseif not getgenv().ancsct then
            for i,v in next, game:GetService('Players'):GetPlayers() do
                v.Character.HumanoidRootPart.Transparency = 1
                v.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
                end
            end
    end)
end)

