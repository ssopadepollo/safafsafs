Pl:Toggle("Spam",function(t)
    getgenv().spam = t
    spawn(function()
        game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
            if key == getgenv().keybindtxt then
        while getgenv().spam == true and wait(getgenv().txtspeed) do
            local args = {
                [1] = getgenv().txt,
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
        end
        end
        end)
    end)
end)

