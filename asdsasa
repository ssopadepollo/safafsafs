                            local v = 10
                            local w =
                                math.round(math.abs((n.Position - u.Position).Magnitude)) / tonumber(v)
                            local x =
                                game:GetService("TweenService"):Create(
                                n,
                                TweenInfo.new(tonumber(w), Enum.EasingStyle.Linear),
                                {CFrame = u}
                            )
