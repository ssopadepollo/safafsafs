Pl:Button("Camera Noclip",function()
    game:GetService("Workspace").Camera.CameraType = "Track"        
end)
Pl:Label("Noclip")


Pl:Textbox("Keybind", true,function(t)
    getgenv().nck = t
    notif("+Added " .. t, 3)
end)

Pl:Button("Noclip",function()
    local plr = game:GetService("Players").LocalPlayer
    local char = plr.Character
    local mouse = game:GetService("Players").LocalPlayer:GetMouse()
    local hum = char:FindFirstChild("HumanoidRootPart")
    mouse.KeyDown:connect(function(key)
        if key == getgenv().nck  then
            loop2 = true
            while loop2 do
                hum.CFrame = hum.CFrame + hum.CFrame.lookVector * 4
                wait()
            end
        end
    end)
    mouse.KeyUp:connect(function(key)
        if key == getgenv().nck then
            loop2 = false
        end
end)
end)

