            local plr = game.Players.LocalPlayer.Text
            if game.Players.LocalPlayer.PlayerGui.SecondaryGui.PlayerList.List[plr].LevelLabel.Text == "100" then
                game:GetService("ReplicatedStorage").Events.Rebirth:FireServer()
